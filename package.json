{
  "name": "proyecto",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node --watch ./bin/www",
    "build": "npm install && cd frontend && npm install --include=dev && npm run build",
    "dev": "concurrently \"npm start\" \"npm run frontend\"",
    "frontend": "cd frontend && npm run dev",
    "test": "set NODE_ENV=test&& jest --runInBand --detectOpenHandles"
  },
  "dependencies": {
    "axios": "^1.13.4",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "express": "~4.16.1",
    "express-validator": "^7.3.0",
    "jest": "^30.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "sequelize": "^6.37.7",
    "slugify": "^1.6.6",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4",
    "swagger": "^0.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}